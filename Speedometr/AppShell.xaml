<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Speedometr.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Speedometr"
    Shell.FlyoutBehavior="Flyout"
    Title="Speedometr">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent 
        Title="Steps"
        ContentTemplate="{DataTemplate local:Stepping}"
        Route="Stepping"/>

</Shell>
